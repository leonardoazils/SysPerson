<div class="row">
    <div class="col col-lg-4 col-md-4 col-sm-6 col-xs-12">
        <div class="form-group form-float">
            <div class="form-line">
                @Html.TextBoxFor(model => model.Cnpj, new { @class = "form-control", @maxlength = "18" })
                <label class="form-label obrigatorio">CNPJ</label>
            </div>
        </div>
    </div>
    <div class="col col-lg-3 col-md-4 col-sm-6 col-xs-12">
        <label class="control-label">Tipo Empresa</label>
        <div class="form-group">
            @Html.DropDownListFor(model => model.TipoEmpresa, "Selecione..", new { @class = "form-control consulta", @Id = "EstadoCivil" })
        </div>
    </div>
</div>
