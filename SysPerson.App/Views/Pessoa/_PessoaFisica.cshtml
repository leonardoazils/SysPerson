<div class="row">
    <div class="col col-lg-4 col-md-4 col-sm-4 col-xs-12">
        <div class="form-group form-float">
            <div class="form-line">
                @Html.TextBoxFor(model => model.Cpf, new { @class = "form-control", @maxlength = "18" })
                <label class="form-label obrigatorio">CPF / CNPJ</label>
            </div>
        </div>
    </div>
</div>
<div class="row">
    <div class="col col-lg-6 col-md-8 col-sm-6 col-xs-12">
        <div class="form-group form-float">
            <div class="form-line">
                @Html.TextBoxFor(model => model.Nome, new { @class = "form-control" })
                <label class="form-label obrigatorio">Nome</label>
            </div>
        </div>
    </div>
    <div class="col col-lg-4 col-md-4 col-sm-6 col-xs-12">
        <div class="form-group form-float">
            <div class="form-line">
                @Html.TextBoxFor(model => model.Email, new { @class = "form-control" })
                <label class="form-label">E-mail</label>
            </div>
        </div>
    </div>
</div>
<div class="row">
    <div class="col col-lg-3 col-md-6 col-sm-6 col-xs-12">
        <label class="control-label">Estado Cívil</label>
        <div class="form-group">
            @Html.DropDownListFor(model => model.EstadoCivil, "Selecione..", new { @class = "form-control consulta", @Id = "EstadoCivil" })
        </div>
    </div>
    <div class="col col-lg-3 col-md-6 col-sm-6 col-xs-12">
        <div class="form-group form-float">
            <div class="form-line">
                @Html.TextBoxFor(model => model.Profissao, new { @class = "form-control" })
                <label class="form-label">Profissão</label>
            </div>
        </div>
    </div>
</div>
<div class="row">
    <div class="col col-lg-4 col-md-4 col-sm-12 col-xs-12">
        <div class="form-group form-float">
            <div class="form-line">
                @Html.TextBoxFor(model => model.NumeroPrincipal, new { @class = "form-control telefone" })
                <label class="form-label">Fone 1</label>
            </div>
        </div>
    </div>
    <div class="col col-lg-4 col-md-4 col-sm-12 col-xs-12">
        <div class="form-group form-float">
            <div class="form-line">
                @Html.TextBoxFor(model => model.NumeroSecundario, new { @class = "form-control telefone" })
                <label class="form-label">Fone 2</label>
            </div>
        </div>
    </div>
    <div class="col col-lg-4 col-md-4 col-sm-12 col-xs-12">
        <div class="form-group form-float">
            <div class="form-line">
                @Html.TextBoxFor(model => model.NumeroReserva, new { @class = "form-control telefone" })
                <label class="form-label">Fone 3</label>
            </div>
        </div>
    </div>
</div>
<div class="row">
    <div class="col col-lg-3 col-md-6 col-sm-6 col-xs-12">
        <label class="control-label">Tipo Empresa</label>
        <div class="form-group">
            @Html.DropDownListFor(model => model.TipoEmpresa, "Selecione..", new { @class = "form-control consulta", @Id = "EstadoCivil" })
        </div>
    </div>
    <div class="col col-lg-3 col-md-6 col-sm-6 col-xs-12">
        <div class="form-group form-float">
            <div class="form-line">
                @Html.TextBoxFor(model => model.Nascimento, new { @class = "form-control" })
                <label class="form-label">Nascimento</label>
            </div>
        </div>
    </div>
    <div class="col col-lg-3 col-md-6 col-sm-6 col-xs-12">
        <label class="control-label">Genêro</label>
        <div class="form-group">
            @Html.DropDownListFor(model => model.Genero, "Selecione..", new { @class = "form-control consulta", @Id = "EstadoCivil" })
        </div>
    </div>
    <div class="col col-lg-3 col-md-6 col-sm-6 col-xs-12">
        <div class="form-group form-float">
            <div class="form-line">
                @Html.TextBoxFor(model => model.Nacionalidade, new { @class = "form-control" })
                <label class="form-label">Nacionalidade</label>
            </div>
        </div>
    </div>
</div>